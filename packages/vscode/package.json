{
	"publisher": "albizures",
	"name": "gleam-ts",
	"displayName": "Gleam TS Plugin",
	"version": "0.0.1",
	"description": "Support import gleam files into typescript",
	"author": "Jose Albizures <jose@albizures.com> (https://albizures.com/)",
	"repository": "https://github.com/albizures/gleam-tools/tree/main/packages/vscode",
	"categories": [
		"Extension Packs"
	],
	"engines": {
		"vscode": "^1.96.0"
	},
	"extensionPack": [
		"gleam.gleam"
	],
	"contributes": {
		"typescriptServerPlugins": [
			{
				"name": "@gleam-tools/ts",
				"enableForWorkspaceTypeScriptVersions": true
			}
		]
	},
	"dependencies": {
		"@gleam-tools/ts": "^0.0.3"
	}
}
